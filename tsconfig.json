{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES5",
    "lib": ["es6", "esnext.asynciterable", "dom"],
    "strict": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "suppressExcessPropertyErrors": false,
    "jsx": "react",
    "inlineSourceMap": true,
    "inlineSources": true
  },
  "exclude": [
    "node_modules",
    "typings/main.d.ts",
    "typings/main",
    "assets",
    "coverage",
    "docs",
    "tmp",
    "uploads"
  ]
}
